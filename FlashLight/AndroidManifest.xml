<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ebs.FlashLight">
    <uses-sdk android:minSdkVersion="18"/>

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".NewPlanet" android:label="@string/app_name" />
        <activity android:name=".TravelActivity" android:label="@string/app_name" />
        <activity android:name=".ConfigActivity" android:label="@string/app_name" />
        <activity android:name=".AttackPlanet" android:label="@string/app_name" />

    </application>
</manifest>
